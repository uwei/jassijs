{
  "compilerOptions": { 
    "baseUrl": "./",
    "allowJs": true,
    //"checkJs":false,
    "target": "es2017",
    "module": "amd",
    "moduleResolution": "node",
    "lib": [
      "es2016", 
      "dom",
      "es5"
    ],
    "inlineSources": true,
    "outFile": "./dist/jassijs-server.js",
    "rootDirs":["./"],
    "paths": {
      //"client/jassijs/server":["./jassijs/server"]
      //"jassijs":["client/jassijs/**/**.*"]
    },
     "sourceMap": true,

    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
  },

  "exclude": [
    "./jassijs/server/**/**.*",
    "./jassijs/modul*.*",
  ],
  "include":[
    "./jassijs/**/**.*",
    "./jassijs/modul*.*",
   "./client/jassijs/server/**/**.*"
],

}