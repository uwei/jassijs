{
  "references": [
    {
      "path":"../jassijs"
    },
    {
      "path":"../jassijs_editor"
    }
  ],
  "compilerOptions": { 
    "baseUrl": "./../",
    "allowJs": true,
    //"checkJs":false,
    "target": "es2017",
    "module": "amd",
    "moduleResolution": "node",
    "lib": [
      "es2016", 
      "dom",
      "es5"
    ],
   
    "skipLibCheck": true,

    "typeRoots": [".././node_modules/@types",".././node_modules2/typescriptservice"],
    "composite": true,
    "inlineSources": true,
    "outFile": "../../dist/jassijs_localserver.js",
    "rootDir": "./../",
    "declaration": true,
     "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
  },
   "include":["./",".././node_modules2", "../jassijs/server/ext", "../jassijs/server/DatabaseSchema.ts"]
 
  
}