{
  "name": "Jassi Ext",
  "description": "provides",
  "version": "0.7",
  "permissions": [
    "debugger",
    "storage",
    "tabs",
    "activeTab",
    "<all_urls>",
    "http://localhost/",
    "http://*/",
    "https://*/",
    "contextMenus"

  ],
  "background": {
    "scripts": ["background.js"]
  },
  "externally_connectable": {
    "matches":  ["*://localhost/*","*://myjassi.herokuapp.com/*"]
  },
  "devtools_page": "devtools.html",
  "browser_action": {
    "default_title": "Jassi Debugger"
  },

 
  "manifest_version": 2    
}
