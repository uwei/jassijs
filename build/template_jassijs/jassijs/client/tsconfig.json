{
  "compilerOptions": { 
    "baseUrl": "./",
    "allowJs": true,
    "target": "es2017",
    "module": "amd",
    "moduleResolution": "node",
    "lib": [
      "esnext", 
      "dom",
      "es5"
    ],
    "typeRoots": [
      "./node_modules/@types",
      "./../node_modules/jassijs/client/node_modules/@types"
  ],
    "rootDir": "./",
    "skipLibCheck": true,
    "sourceMap": true,
    "outDir": "./js", 
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
  },
  "exclude": [
    "js",
    "jassistart.js",
    "jassichrome",
    "jassijs/service-worker.js",
    "jassijs.js"
  ] 
}