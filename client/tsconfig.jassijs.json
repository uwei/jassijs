{
  "compilerOptions": { 
    "baseUrl": "./jassijs",
    "allowJs": true,
    //"checkJs":false,
    "target": "es2017",
    "module": "amd",
    "moduleResolution": "node",
    "lib": [
      "es2016", 
      "dom",
      "es5"
    ],
    "inlineSources": true,
    "outFile": ".././dist/jassijs.js",
    "rootDir": "./",

     "sourceMap": true,

    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
  },
  "exclude": [
    "jassijs/service-worker.js"
  ],
  "include":["./jassijs"],
  
}